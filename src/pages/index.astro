---
import Layout from '~/layouts/PageLayout.astro';

import Hero from '~/components/widgets/Hero.astro';
// import Note from '~/components/widgets/Note.astro';
import Features from '~/components/widgets/Features.astro';
// import Features2 from '~/components/widgets/Features2.astro';
import Steps from '~/components/widgets/Steps.astro';
import Content from '~/components/widgets/Content.astro';
// import BlogLatestPosts from '~/components/widgets/BlogLatestPosts.astro';
import FAQs from '~/components/widgets/FAQs.astro';
import Stats from '~/components/widgets/Stats.astro';
// import CallToAction from '~/components/widgets/CallToAction.astro';

const metadata = {
  title: 'AstroWind — Free template for creating websites with Astro + Tailwind CSS',
  ignoreTitleTemplate: true,
};
---

<Layout metadata={metadata}>
  <!-- Hero Widget ******************* -->

  <Hero
    actions={[
      {
        variant: 'primary',
        text: 'Get in touch',
        href: 'https://ganeswarv.me',
        target: '_blank',
      },
      { text: 'Learn more', href: '#features' },
    ]}
    image={{ src: '~/assets/images/hero-image.png', alt: 'AstroWind Hero Image' }}
  >
    <Fragment slot="title">
      [Company Name]
    </Fragment>

    <Fragment slot="subtitle">
      <span class="hidden sm:inline">
        <span class="font-semibold">[Company Name]</span> is a one-stop solution for schools, providing advanced ERP software, books, custom websites, high-quality uniforms, stationery and expert consultancy. We streamline school operations, enhance learning experiences and support educational institutions with innovative, cost-effective and tailored solutions.</span
      >
      <span class="block mb-1 sm:hidden font-bold text-blue-600">AstroWind: Production-ready.</span>
      
    </Fragment>
  </Hero>

  <!--Stats -->
  <Stats
  stats={[
    { title: 'Schools', amount: '100+' },
    { title: 'schools', amount: '100+' },
    { title: 'Students', amount: '10,000+' },
    { title: 'Users', amount: '20,000+' },
  ]}
/>

  <!-- Features Widget *************** -->

  <Features
    id="Products"
    tagline="Our Products"
    title="What you get with [Company Name]"
    subtitle="At [Company Name], we provide complete solutions to simplify school operations and enhance efficiency—all in one place! "
    items={[
      {
        title: 'ERP Application',
        description:
          'A school ERP app streamlines operations like attendance, fees, results, assignments and communication, enhancing efficiency, transparency and productivity.',
        icon: 'tabler:server-cog',
      },
      {
        title: 'Books',
        description:
          "We provide a wide range of textbooks, reference materials and workbooks tailored to various curricula. Our selection ensures quality, affordability and accessibility for effective learning.",
        icon: 'tabler:books',
      },
      {
        title: 'Website',
        description:
          'We create custom, mobile-friendly school websites that reflect your school’s unique identity. Designed for seamless navigation and clear communication, our websites help you connect with parents, students and staff effortlessly while showcasing your school’s vision and achievements.',
        icon: 'tabler:world-www',
      },
      {
        title: 'Uniforms',
        description:
          'We offer high-quality, durable school uniforms that reflect your school’s ethos. Available in various sizes, our uniforms combine comfort, functionality and pride for students, ensuring they feel confident and ready to excel.',
        icon: 'tabler:shirt',
      },
      {
        title: 'Stationery',
        description:
          'We provide a premium range of high-quality stationery essentials for schools including notebooks, pens, art supplies and more. Designed to combine durability, affordability and seamless learning experiences.',
        icon: 'tabler:pencil',
      },
      
      {
        title: 'Consultancy',
        description:
          'We offer expert consultancy services to schools, guiding them in academic planning, administration and technology integration. Our solutions enhance efficiency, compliance and overall institutional growth.',
        icon: 'tabler:messages',
      },
    ]}
  />

  <!-- Content Widget **************** -->

  
  <!-- Content Widget **************** -->
  <Content
  isReversed
  tagline="Core Benefits"
  title="[Company Name]'s Benefits!"
  items={[
    {
      title: 'Integrated Services',
      description:
        'Schools can access ERP, websites, uniforms, stationery, books and consultancy under one roof, simplifying management.',
    },
    {
      title: 'Enhanced Efficiency',
      description:
        'Our ERP system automates attendance, fee payments, payrolls and more, reducing administrative workload.',
    },
    {
      title: 'Customization',
      description:
        'Tailored solutions for ERP software, uniforms and stationery to match each school’s unique identity and requirements.',
    },
    {
      title: 'Time Saving',
      description:
        'Schools can focus on education while we handles logistics, supplies and digital transformation.',
    },
  ]}
  image={{
    src: 'https://images.unsplash.com/photo-1519389950473-47ba0277781c?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=2070&q=80',
    alt: 'Colorful Image',
  }}
>


  <Fragment slot="bg">
    <div class="absolute inset-0 bg-blue-50 dark:bg-transparent"></div>
  </Fragment>
</Content>

  <Content
    isAfterContent
    items={[
      {
        title: 'Cost-Effective Solutions',
        description: `Our products and services offer exceptional value, combining affordability with top-tier quality to meet your school’s needs without stretching your budget.`,
      },
      {
        title: 'Improved Communication',
        description:
          'ERP features like digital diaries, announcements and complaints enhance engagement between parents, teachers and students.',
      },
      {
        title: 'Academic Excellence',
        description:
          "Quality books, structured teaching plans and expert consultancy help schools improve learning outcomes.",
      },
      {
        title: 'Professional Branding',
        description:
          'We help schools build a distinct and impactful identity through tailored branding solutions from uniforms to websites, ensuring your school stands out with pride and professionalism.',
      },
    ]}
    image={{
      src: 'https://images.unsplash.com/photo-1600132806370-bf17e65e942f?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=2194&q=80',
      alt: 'Blueprint Image',
    }}
  >
    

    <Fragment slot="bg">
      <div class="absolute inset-0 bg-blue-50 dark:bg-transparent"></div>
    </Fragment>
  </Content>

  <!-- Content Widget **************** -->

  <Content
    isReversed
    isAfterContent
    items={[
      {
        title: 'Scalability and Growth Support',
        description:
          'Our solutions grow with schools, adapting to their evolving needs, whether expanding campuses or upgrading systems.',
      },
      {
        title: 'Regularity Compilance',
        description:
          'Our consultancy ensures schools meet educational regulations, policies and best practices effortlessly.',
      },
      {
        title: 'Data Driven Decision Making',
        description:
          'The ERP software provides insights through reports and analytics, helping schools make informed decisions.',
      },
      {
        title: 'Reliabale Support and Maintenace',
        description: `Ongoing technical support and consultation ensure smooth operations with minimal disruptions.`,
      },
    ]}
    image={{
      src: 'https://images.unsplash.com/photo-1611462985358-60d3498e0364?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=2070&q=80',
      alt: 'Astronauts Image',
    }}
  >


    <Fragment slot="bg">
      <div class="absolute inset-0 bg-blue-50 dark:bg-transparent"></div>
    </Fragment>
  </Content>


  


  <!-- Steps Widget ****************** -->

  <Steps
    title="Get The Most Advanced EdTech Services From Us."
    items={[
      {
        title: 'Step 1: <span class="font-medium">Connect With Us</span>',
        description:
          "Reach out to us to discuss your school’s needs. Whether you require an advanced ERP, high-quality books, custom uniforms, a professional website, essential stationery or expert consultancy, we’re here to help.",
        icon: 'tabler:phone',
      },
      {
        title: 'Step 2: <span class="font-medium">Share Your Requirements</span>',
        description:
          "Tell us what your school needs—whether it’s an ERP software, books, uniforms, website, stationery or consultancy. Our team will analyze your needs and suggest the best solutions.",
        icon: 'tabler:mail',
      },
      {
        title: 'Step 3: <span class="font-medium">Get A Customized Plan</span>',
        description:
          'Based on your requirements, we’ll create a tailored package for your school, ensuring a perfect fit for your school’s goals.',
        icon: 'tabler:receipt',
      },
      {
        title: 'Step 4: <span class="font-medium">Confirm And Begin</span>',
        description:"Once you're satisfied with the plan, we’ll start the process. From ERP setup to resource delivery, we handle everything smoothly.",
        icon: 'tabler:rocket',
      },
      {
        title: 'Step 5: <span class="font-medium">Ongoing Support And Growth</span>',
        description:"We stay with you beyond the setup, providing continuous support, updates, and expert advice to ensure your school runs efficiently.",
        icon: 'tabler:graph',
      },
    ]}
    image={{
      src: 'https://images.unsplash.com/photo-1616198814651-e71f960c3180?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=987&q=80',
      alt: 'Steps image',
    }}
  />
  
  <!-- Features 3 widget ****************** -->
  <section class="text-center py-12">
    <h1 class="text-3xl font-bold text-gray-900">Smart Solutions to Simplify and Elevate Your School!</h1>    
    <div class="grid grid-cols-1 md:grid-cols-3 gap-8 mt-8 max-w-6xl mx-auto px-4">
      
      <!-- Simple & Easy to Use -->
      <div class="flex flex-col items-center text-center">
        <img src= "data:image/svg+xml;base64,%%2BCiAgICA8L2c%%%2BCjwvc3ZnPgo%3D", width="125px", height="125px">
        <h3 class="text-lg font-semibold mt-4">Effortless & User-Friendly for Educators</h3>
        <p class="text-gray-600 mt-2">Built to connect teachers and parents seamlessly. No technical skills needed to get things done quickly.
        </p>
      </div>
      
      <!-- Powerful Features -->
      <div class="flex flex-col items-center text-center">
        <img src="data:image/svg+xml;base64,%%%2BCiAgICA8L2c%%2BCiAgICAgICAgPHBhdGggaWQ9IlBhdGhfMTY2IiBkYXRhLW5hbWU9IlBhdGggMTY2IiBkPSJNMTAxMS41LDYxNS4xNjdoLTEwLjQxN2EyLjA4MywyLjA4MywwLDAsMSwwLTQuMTY3SDEwMTEuNWEyLjA4MywyLjA4MywwLDAsMSwwLDQuMTY3WiIgdHJhbnNmb3JtPSJ0cmFuc2xhdGUoLTk5OSAtNjExKSIgZmlsbD0iIzM0NDk1ZSIvPgogICAgICA8L2c%2BCiAgICA8L2c%2BCiAgICA8ZyBpZD0iR3JvdXBfMjg3IiBkYXRhLW5hbWU9Ikdyb3VwIDI4NyIgdHJhbnNmb3JtPSJ0cmFuc2xhdGUoMzIuODIxKSI%%2BCiAgICA8L2c%2BCiAgPC9nPgo8L3N2Zz4K", width="125px", height="125px">
        <h3 class="text-lg font-semibold mt-4">Advanced Tools for Your School</h3>
        <p class="text-gray-600 mt-2">Get everything you need to efficiently manage your school—ready to use from day one.</p>
      </div>
      
      <!-- Online & On-site Support -->
      <div class="flex flex-col items-center text-center">
        <img src="data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHhtbG5zOnhsaW5rPSJodHRwOi8vd3d3LnczLm9yZy8xOTk5L3hsaW5rIiB3aWR0aD0iOTYiIGhlaWdodD0iODIiIHZpZXdCb3g9IjAgMCA5NiA4MiI%2BDQogIDxkZWZzPg0KICAgIDxjbGlwUGF0aCBpZD0iY2xpcC1wYXRoIj4NCiAgICAgIDxyZWN0IGlkPSJSZWN0YW5nbGVfMTA3MiIgZGF0YS1uYW1lPSJSZWN0YW5nbGUgMTA3MiIgd2lkdGg9Ijk2IiBoZWlnaHQ9IjgyIiB0cmFuc2Zvcm09InRyYW5zbGF0ZSgtMC40NjUpIiBmaWxsPSJub25lIi8%%2BDQo%3D", width="125px", height="125px"> 
        <h3 class="text-lg font-semibold mt-4">Reliable Online & On-Site Support</h3>
        <p class="text-gray-600 mt-2">In addition to on-site and online assistance, we provide on-demand training and webinar sessions for seamless onboarding.</p>
      </div>
    </div>
  </section>
  
